{
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": ["error", 4],
        "comma-dangle": ["error", "never"]
    },
    "globals": {
        "jQuery": true,
        "localStorage": true,
        "describe": true,
        "it": true,
        "beforeEach": true,
        "afterEach": true,
        "expect": true,
        "jasmine": true,
        "spyOn": true
    }
}